
// Classe pai
class Animal {
    // Atributos
    String nome;
    String raca;

    // Construtor
    public Animal(String nome, String raca) {
        this.nome = nome;
        this.raca = raca;
    }

    // M√©todo gen√©rico
    public void comer() {
        System.out.println(nome + " da ra√ßa " + raca + " est√° comendo.");
    }

    // M√©todo gen√©rico (pode ser sobrescrito pelas classes filhas)
    public void andar() {
        System.out.println(nome + " da ra√ßa " + raca + " est√° andando.");
    }

    // M√©todo gen√©rico (pode ser sobrescrito)
    public void voar() {
        System.out.println(nome + " da ra√ßa " + raca + " est√° tentando voar...");
    }
}

// Classe filha: Cachorro
class Cachorro extends Animal {
    public Cachorro(String nome, String raca) {
        super(nome, raca);
    }

    public void latir() {
        System.out.println(nome + " est√° latindo: Au au!");
    }

    // Sobrescrevendo andar
    @Override
    public void andar() {
        System.out.println(nome + " da ra√ßa " + raca + " est√° correndo animado!");
    }
}

